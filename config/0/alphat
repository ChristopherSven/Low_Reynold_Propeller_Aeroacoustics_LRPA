FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];


internalField   uniform 0.001;

boundaryField
{
    outlet
    {
        type            calculated;
        value           uniform 0.001;
    }
    propeller
    {
        type            compressible::alphatWallFunction;
        value           uniform 0.001;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 0.001;
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 0.001;
    }
}


// ************************************************************************* //
