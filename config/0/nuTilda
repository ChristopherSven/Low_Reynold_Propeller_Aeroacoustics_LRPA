FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 4.686e-5;

boundaryField
{
    outlet
    {
        type            fixedValue;
        value           4.686e-5;
    }
    propeller
    {
        type            fixedValue;
        value           4.686e-5;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 4.686e-5;
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 4.686e-5;
    }
}


