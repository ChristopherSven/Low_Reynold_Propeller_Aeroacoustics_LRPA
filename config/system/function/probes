// For fixing the probes to the mesh
probesFixed
{
        type    probes;
        libs    ("libsampling.so");
        writeControl    timeStep;
        writeInterval   $monitor;
        fields (p);
        probeLocations
        (
                // directivity plot probes (-30,-20,-10,0,10,20,30) at 1.2R
                (0 0.1241880429026885 -0.07169999999999999)
                (0 0.13475192182069926 -0.049045688552900896)
                (0 0.14122143178195062 -0.02490114867743781)
                (0 0.1434 0.0)
                (0 0.14122143178195062 0.02490114867743781)
                (0 0.13475192182069926 0.049045688552900896)
                (0 0.1241880429026885 0.07169999999999999)
                


        );
        fixedLocations true;
}

