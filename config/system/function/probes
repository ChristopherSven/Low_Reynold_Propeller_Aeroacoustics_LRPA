// Fixing the probe the absolute coordinates 
probes
{
        type    probes;
        libs    ("libsampling.so");
        writeControl    timeStep;
        writeInterval   $monitor;
        fields (p);
        probeLocations
        (
                // Suction surface
                (0 0.05975 0.05975) 
                (0 0.0956 0.05975)
                (0 0.11471999999999999 0.05975)
                (0 -0.05975 0.05975) 
                (0 -0.0956 0.05975)
                (0 -0.11471999999999999 0.05975)

                //pressure surface
                (0 0.05975 -0.05975) 
                (0 0.0956 -0.05975)
                (0 0.11471999999999999 -0.05975)
                (0 -0.05975 -0.05975) 
                (0 -0.0956 -0.05975)
                (0 -0.11471999999999999 -0.05975)
        );
        fixedLocations false; 
}

